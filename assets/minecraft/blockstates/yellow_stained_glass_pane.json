{
    "multipart": [
        {   "apply": { "model": "glass/pane/stained/yellow/post_ends" }},
        {   "when": { "OR": [
            {"north": true, "east": true, "south": true},
            {"north": true, "east": true, "west": true},
            {"east": true, "south": true, "west": true},
            {"north": true, "south": true, "west": true},
            {"north": false, "east": false, "south": false},
            {"north": false, "east": false, "west": false},
            {"east": false, "south": false, "west": false},
            {"north": false, "south": false, "west": false}
        ]},
            "apply": { "model": "glass/pane/stained/yellow/post" }
        },
        {   "when": { "north": true },
            "apply": { "model": "glass/pane/stained/yellow/side" }
        },
        {   "when": { "east": true },
            "apply": { "model": "glass/pane/stained/yellow/side", "y": 90 }
        },
        {   "when": { "south": true },
            "apply": { "model": "glass/pane/stained/yellow/side", "y": 180 }
        },
        {   "when": { "west": true },
            "apply": { "model": "glass/pane/stained/yellow/side", "y": 270 }
        }
    ]
}
